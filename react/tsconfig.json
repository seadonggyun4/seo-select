{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["DOM", "DOM.Iterable", "ES6"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false,
    "jsx": "react-jsx",
    "declaration": true,
    "declarationMap": true,
    "outDir": "./dist",
    "rootDir": "./wrapper",
    "baseUrl": "./wrapper",
    "paths": {
      "../../dist/types/index.js": ["../../dist/types/index.d.ts"],
      "../../dist/index.js": ["../../dist/index.d.ts"],
      "../../dist/components/seo-select-search/index.js": ["../../dist/components/seo-select-search/index.d.ts"]
    }
  },
  "include": [
    "wrapper/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}