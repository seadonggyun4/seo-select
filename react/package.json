{
  "name": "seo-select-react",
  "version": "1.1.21",
  "description": "React wrapper components for seo-select - A customizable and accessible select component with search functionality",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./SeoSelect": {
      "types": "./dist/SeoSelect.d.ts",
      "import": "./dist/SeoSelect.js",
      "require": "./dist/SeoSelect.cjs"
    },
    "./SeoSelectSearch": {
      "types": "./dist/SeoSelectSearch.d.ts",
      "import": "./dist/SeoSelectSearch.js",
      "require": "./dist/SeoSelectSearch.cjs"
    }
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "keywords": [
    "react",
    "select",
    "dropdown",
    "search",
    "accessible",
    "web-components",
    "lit",
    "typescript",
    "virtual-scroll",
    "multi-select"
  ],
  "author": "seo dong gyun <seadonggyun@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seadonggyun4/seo-select",
    "directory": "react"
  },
  "bugs": {
    "url": "https://github.com/seadonggyun4/seo-select/issues"
  },
  "homepage": "https://seo-select.netlify.app/",
  "scripts": {
    "build": "npm run clean && npm run build:lib",
    "build:lib": "vite build && npm run build:types",
    "build:types": "tsc --project ./tsconfig.json --emitDeclarationOnly --skipLibCheck",
    "clean": "rm -rf dist",
    "dev": "vite",
    "type-check": "tsc --project ./tsconfig.json --noEmit --skipLibCheck",
    "prepublishOnly": "npm run type-check && npm run build",
    "publish:npm": "npm publish --access public"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "seo-select": ">=2.2.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/node": "^22.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "seo-select": ">=2.2.0",
    "typescript": "^5.0.0",
    "tsx": "^4.19.2",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.3.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "sideEffects": false
}
