{
  "name": "seo-select",
  "version": "2.0.14",
  "description": "A customizable and accessible select component with search functionality built with Lit",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "jsdelivr": "./dist/index.js",
  "unpkg": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./components/seo-select": {
      "types": "./src/components/seo-select/index.ts",
      "import": "./src/components/seo-select/index.ts"
    },
    "./components/seo-select-search": {
      "types": "./src/components/seo-select-search/index.ts",
      "import": "./src/components/seo-select-search/index.ts"
    },
    "./event": {
      "types": "./src/event/index.ts",
      "import": "./src/event/index.ts"
    },
    "./dist": "./dist/index.js"
  },
  "files": [
    "src/**/*",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "lit",
    "web-components",
    "select",
    "dropdown",
    "search",
    "accessible",
    "virtual-scroll",
    "typescript",
    "react",
    "vue",
    "angular",
    "cdn",
    "esm"
  ],
  "author": "seo dong gyun <seadonggyun@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seadonggyun4/seo-select"
  },
  "bugs": {
    "url": "https://github.com/seadonggyun4/seo-select/issues"
  },
  "homepage": "https://seo-select.netlify.app/",
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:cdn && npm run inline:css",
    "build:cdn": "vite build --mode production",
    "build:types": "tsc -p tsconfig.build.json --emitDeclarationOnly",
    "build:check": "tsc -p tsconfig.build.json --noEmit",
    "build:docs": "node scripts/build-docs.js",
    "clean": "rm -rf dist",
    "inline:css": "node scripts/inline-css.js",
    "release": "bash scripts/release.sh",
    "release:patch": "bash scripts/auto-release.sh patch",
    "release:minor": "bash scripts/auto-release.sh minor",
    "release:major": "bash scripts/auto-release.sh major",
    "dev": "vite",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run build && npm run type-check"
  },
  "dependencies": {
    "lit": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.1.0",
    "sass": "^1.89.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.6",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "fs-extra": "^11.2.0"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
