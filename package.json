{
  "name": "seo-select",
  "version": "4.3.7",
  "description": "Customizable, high-performance select dropdown Web Component with search, virtual scroll, multiple selection, i18n, and framework compatibility (React, Vue, Svelte, Angular, Qwik). Accessible, SSR-safe, and zero dependencies.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./components/seo-select": {
      "types": "./dist/components/seo-select/index.d.ts",
      "import": "./dist/components/seo-select/index.js"
    },
    "./components/seo-select-search": {
      "types": "./dist/components/seo-select-search/index.d.ts",
      "import": "./dist/components/seo-select-search/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js"
    },
    "./constants": {
      "types": "./dist/constants/constants.d.ts",
      "import": "./dist/constants/constants.js"
    },
    "./styles": {
      "types": "./dist/styles/style.d.ts",
      "import": "./dist/styles/components/style.css"
    },
    "./react": {
      "types": "./dist/wrappers/react/index.d.ts",
      "import": "./dist/wrappers/react/index.js"
    },
    "./vue": {
      "types": "./dist/wrappers/vue/index.d.ts",
      "import": "./dist/wrappers/vue/index.js"
    },
    "./angular": {
      "types": "./dist/wrappers/angular/index.d.ts",
      "import": "./dist/wrappers/angular/index.js"
    },
    "./solid": {
      "types": "./dist/wrappers/solid/index.d.ts",
      "import": "./dist/wrappers/solid/index.js"
    },
    "./qwik": {
      "types": "./dist/wrappers/qwik/index.d.ts",
      "import": "./dist/wrappers/qwik/index.js"
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "select",
    "dropdown",
    "web component",
    "web-components",
    "custom element",
    "virtual scroll",
    "virtualized",
    "search",
    "autocomplete",
    "multiselect",
    "multi-select",
    "multiple select",
    "combobox",
    "tag input",
    "form",
    "input",
    "i18n",
    "internationalization",
    "accessible",
    "a11y",
    "ssr",
    "ssr safe",
    "react",
    "vue",
    "svelte",
    "angular",
    "qwik",
    "solid",
    "typescript",
    "lit",
    "framework agnostic",
    "framework compatible",
    "customizable",
    "theme",
    "dark mode",
    "lightweight",
    "performance",
    "fast",
    "dropdown menu"
  ],
  "author": "seo dong gyun <seadonggyun@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seadonggyun4/seo-select"
  },
  "bugs": {
    "url": "https://github.com/seadonggyun4/seo-select/issues"
  },
  "homepage": "https://seo-select.netlify.app/",
  "scripts": {
    "build": "npm run clean && npm run build:lib && npm run build:types && npm run build:wrapper-types",
    "build:lib": "vite build --mode production",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "build:wrapper-types": "cp src/wrappers/react/index.d.ts dist/wrappers/react/ && cp src/wrappers/vue/index.d.ts dist/wrappers/vue/ && cp src/wrappers/angular/index.d.ts dist/wrappers/angular/ && cp src/wrappers/solid/index.d.ts dist/wrappers/solid/ && cp src/wrappers/qwik/index.d.ts dist/wrappers/qwik/ && mkdir -p dist/styles && cp src/styles/style.d.ts dist/styles/",
    "build:min": "vite build --config vite.config.min.ts",
    "build:demo": "tsx scripts/build-demo.ts",
    "clean": "rm -rf dist min",
    "release": "bash scripts/release.sh",
    "release:patch": "bash scripts/release.sh patch",
    "release:minor": "bash scripts/release.sh minor",
    "release:major": "bash scripts/release.sh major",
    "dev": "vite --config vite.config.demo.ts",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "npm run type-check && npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/react": "^18.2.0",
    "sass": "^1.89.2",
    "terser": "^5.44.1",
    "tsx": "^4.19.2",
    "typescript": "~5.8.3",
    "vite": "^7.0.6"
  },
  "peerDependencies": {
    "@angular/core": ">=14.0.0",
    "@builder.io/qwik": ">=1.0.0",
    "react": ">=17.0.0",
    "solid-js": ">=1.0.0",
    "typescript": ">=4.0.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "@angular/core": {
      "optional": true
    },
    "solid-js": {
      "optional": true
    },
    "@builder.io/qwik": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
